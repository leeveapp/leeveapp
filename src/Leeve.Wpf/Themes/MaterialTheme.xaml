<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:material="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:converters="clr-namespace:Leeve.Wpf.Common.Converters">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/Themes/Theme.Primary.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Themes/Theme.Accent.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Themes/BorderStyles.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Themes/CustomButtonStyle.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Themes/Icons.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Themes/ListItemStyle.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Themes/SelectionCardStyle.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Themes/PopupBoxStyles.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Themes/TransitionSlideStyles.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <material:BrushOpacityConverter x:Key="BrushOpacityConverter" />
    <converters:BorderClipConverter x:Key="BorderClipConverter" />
    <material:BrushRoundConverter x:Key="BrushRoundConverter" />
</ResourceDictionary>