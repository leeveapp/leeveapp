<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <Color
        x:Key="SecondaryHueLightColor">
        #FFD770
    </Color>
    <Color
        x:Key="SecondaryHueMidColor">
        #F4A460
    </Color>
    <Color
        x:Key="SecondaryHueDarkColor">
        #BD7705
    </Color>
    <Color
        x:Key="SecondaryLightForegroundColor">
        #FFF4E9
    </Color>
    <Color
        x:Key="SecondaryDarkForegroundColor">
        #070200
    </Color>
    <SolidColorBrush
        x:Key="SecondaryHueLightBrush"
        Color="#FFD770"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="SecondaryHueMidBrush"
        Color="#F4A460"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="SecondaryHueDarkBrush"
        Color="#BD7705"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="SecondaryHueDarkForegroundBrush"
        Color="#FFF4E9"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="SecondaryHueMidForegroundBrush"
        Color="#FFF4E9"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="SecondaryHueLightForegroundBrush"
        Color="#070200"
        po:Freeze="True" />
</ResourceDictionary>